    "nock": "^13.0.5",
    "node-fetch": "^2.6.1",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
